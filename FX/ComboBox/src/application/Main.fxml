<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <ComboBox fx:id="comboBox" layoutX="138.0" layoutY="185.0" onAction="#comboChanged" prefHeight="30.0" prefWidth="150.0" promptText="Select Name" />
      <Label fx:id="myLabel" layoutX="189.0" layoutY="145.0" text="Label">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="button" layoutX="158.0" layoutY="252.0" mnemonicParsing="false" onAction="#buttonActived" prefHeight="37.0" prefWidth="110.0" text="Button" />
   </children>
</AnchorPane>
