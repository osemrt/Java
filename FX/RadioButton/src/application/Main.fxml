<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="243.0" minWidth="225.0" prefHeight="243.0" prefWidth="225.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <RadioButton fx:id="rb1" layoutX="42.0" layoutY="52.0" mnemonicParsing="false" onAction="#radioSelected" text="Female" textAlignment="CENTER">
         <toggleGroup>
            <ToggleGroup fx:id="Group" />
         </toggleGroup>
         <font>
            <Font size="24.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rb2" layoutX="42.0" layoutY="103.0" mnemonicParsing="false" onAction="#radioSelected" text="Male" textAlignment="CENTER" toggleGroup="$Group">
         <font>
            <Font size="24.0" />
         </font>
      </RadioButton>
      <Label fx:id="label" layoutX="66.0" layoutY="159.0" onDragDetected="#radioSelected" text="Label" textAlignment="CENTER">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
